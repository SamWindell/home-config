            {
                "language": "objc",
                "path": "./snippets/objc.json"
            },
            {
                "language": "loremipsum",
                "path": "./snippets/loremipsum.json"
            },
            {
                "language": "cmake",
                "path": "./snippets/cmake.json"
            }
        ]
